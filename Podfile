# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SpeechPathApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SpeechPathApp

  # google analytics pod
  pod 'Firebase/Analytics'

  # auth pod
  pod 'Firebase/Auth'

  # Firestore pod
  pod "Firebase/Firestore"

  # core pod
  pod 'Firebase/Core'

  # database pod
  pod 'Firebase/Database'

  target 'SpeechPathAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SpeechPathAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
